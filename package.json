{
    "type": "module",
    "name": "@dakefujo/prettier-config",
    "version": "1.1.0",
    "description": "Prettier config.",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "author": "Dake Fujo",
    "license": "MIT",
    "keywords": [
        "prettier",
        "config",
        "css",
        "scss"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dakefujo/prettier-config.git"
    },
    "packageManager": "pnpm@9.15.4",
    "scripts": {
        "build": "unbuild",
        "pub": "pnpm build && npm publish --access=public"
    },
    "peerDependencies": {
        "prettier": "^3.0.0",
        "vue-tsc": "~2.1.0 || ~2.2.0"
    },
    "peerDependenciesMeta": {
        "vue-tsc": {
            "optional": true
        }
    },
    "dependencies": {
        "prettier-plugin-organize-imports": "^4.1.0"
    },
    "devDependencies": {
        "@dakefujo/ts-config": "^1.0.0",
        "typescript": "^5.7.3",
        "unbuild": "^3.3.1",
        "vue-tsc": "^2.2.0"
    }
}
